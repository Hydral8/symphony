{
  "base_branch": "main",
  "branch_prefix": "world",
  "worlds_dir": "/tmp/parallel_worlds_worlds",
  "default_world_count": 3,
  "runner": {
    "command": "python3 .parallel_worlds/runner_auto.py",
    "timeout_sec": 300
  },
  "codex": {
    "enabled": true,
    "command": "codex exec --model gpt-5.3-codex --full-auto --skip-git-repo-check --color never",
    "timeout_sec": 900
  },
  "render": {
    "command": "python3 .parallel_worlds/render_auto.py",
    "timeout_sec": 180,
    "preview_lines": 25
  },
  "execution": {
    "max_parallel_worlds": 4,
    "workspace_mode": "worktree"
  },
  "strategies": [
    {
      "name": "minimal-fix",
      "notes": "Smallest targeted change with low risk."
    },
    {
      "name": "robust-fix",
      "notes": "Root-cause oriented implementation with guardrails."
    },
    {
      "name": "refactor-path",
      "notes": "Refactor to improve maintainability while solving intent."
    }
  ]
}
